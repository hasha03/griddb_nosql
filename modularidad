        <script>
    function factorial (nu) {
	var total = 1; 
	for (i=1; i<=nnu; i++) {
		total = total * i; 
	}
	return total;       
    }
         var nro = Number(prompt("ingrese numero "));
         
         var fact = factorial(nro);
         
         document.write("El factorial de "+nro+" es: "+fact);
            </script>    
